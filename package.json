{
  "name": "responsive-gsap",
  "version": "1.0.1",
  "description": "A TypeScript utility package",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "pnpm run clean && tsup",
    "clean": "rm -rf dist",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepublishOnly": "pnpm run build"
  },
  "keywords": [
    "typescript",
    "utility",
    "javascript",
    "responsive-gsap"
  ],
  "author": {
    "name": "phucbm",
    "url": "https://github.com/phucbm"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/phucbm/responsive-gsap"
  },
  "bugs": {
    "url": "https://github.com/phucbm/responsive-gsap/issues"
  },
  "homepage": "https://github.com/phucbm/responsive-gsap",
  "dependencies": {
    "@gsap/react": "^2.1.2",
    "gsap": "^3.13.0",
    "react": "^19.2.0"
  },
  "devDependencies": {
    "@phucbm/banner": "^0.0.2",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.10.0",
    "@types/react": "^19.2.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "ts-jest": "^29.4.5",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=16"
  },
  "sideEffects": false
}